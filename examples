<script>
    // changes the text of the pro button
    const changeProButton = function(proLevel) {
        if(proLevel <= 1){
            document.getElementById('pro-button').innerHTML = "Upgrade to Pro";
        }
        else{
            document.getElementById('pro-button').innerHTML = "Pro";
        }
    };

</script>
